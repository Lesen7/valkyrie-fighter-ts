{
    "extends": [
      "tslint:latest"
    ],
    "rules": {
      "member-access": [
        true,
        "no-public"
      ],
      "no-console": false,
      "ordered-imports": false,
      "object-literal-sort-keys": false,
      "no-implicit-dependencies": [
        true,
        "dev"
      ],
      "trailing-comma":false,
      "quotemark": [true, "single"],
      "interface-name": true,
      "member-ordering": false
    }
  }